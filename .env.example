# .env
# Environment variables for Docker Compose services.
# DO NOT commit this file to version control in production environments.
# Replace values with secure, strong credentials in production.

# SQL Server
SA_PASSWORD=YourStrong!Password123

# ASP.NET Core environment
ASPNETCORE_ENVIRONMENT=Development

# RabbitMQ (default guest account - change in production)
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest

# Redis - no password by default (secure in production!)
REDIS_PASSWORD=

# Prometheus scrape endpoint path (used in ASP.NET Core metrics exporter)
PROMETHEUS_METRICS_PATH=/metrics

# Optional - customize these if needed
WEBAPI_PORT=5000
FRONTEND_PORT=80
